<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="student" class="me.akaada.model.Student">
        <constructor-arg name="userName" value="lee"></constructor-arg>
        <constructor-arg name="password" value="1234"></constructor-arg>
     </bean>


    <bean id="student1" class="me.akaada.model.Student">
        <constructor-arg index="0" value="lee" type="java.lang.String"></constructor-arg>
        <constructor-arg index="1" value="10" type="int"></constructor-arg>
    </bean>

</beans>